{
  "name": "style9",
  "version": "0.9.1",
  "description": "CSS-in-JS compiler",
  "author": "Johan Holmerin <johan@holmer.in>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/johanholmerin/style9.git"
  },
  "engines": {
    "node": ">=12"
  },
  "types": "./types/index.d.ts",
  "keywords": [
    "styles",
    "css",
    "css-in-js",
    "babel",
    "babel-plugin",
    "gatsby",
    "gatsby-plugin",
    "next",
    "nextjs",
    "nextjs-plugin",
    "webpack",
    "rollup",
    "rollup-plugin"
  ],
  "dependencies": {
    "@babel/core": "^7.10.4",
    "@babel/types": "^7.8.3",
    "@rollup/pluginutils": "^3.0.4",
    "csstype": "^3.0.6",
    "inline-style-expand-shorthand": "^1.2.0",
    "known-css-properties": "^0.19.0",
    "loader-utils": "1.4.0",
    "murmurhash-js": "^1.0.0",
    "postcss": "^7.0.32",
    "postcss-discard-duplicates": "^4.0.2",
    "postcss-selector-parser": "^6.0.2",
    "sort-css-media-queries": "^1.5.0",
    "webpack-sources": "^2.2.0",
    "webpack-virtual-modules": "^0.4.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "babel-jest": "^26.1.0",
    "babel-plugin-tester": "^10.0.0",
    "dtslint": "^4.0.7",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "next": "link:examples/nextjs/node_modules/next",
    "prettier": "2.2.1",
    "rollup": "^1.29.0",
    "typescript": "^4.2.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "scripts": {
    "test": "jest",
    "test:types": "dtslint types",
    "test:examples": "./scripts/test-examples.sh",
    "lint": "eslint '**/*.js' --fix",
    "format": "prettier '**/*.{js,ts}' --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "npm run format"
    ],
    "*.ts": [
      "npm run format"
    ]
  }
}
